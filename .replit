modules = ["web", "python-3.12"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd JaredL23.github.io && python -m http.server 5000 --bind 0.0.0.0"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 46687
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["python", "-m", "http.server", "5000", "--bind", "0.0.0.0", "--directory", "JaredL23.github.io"]
